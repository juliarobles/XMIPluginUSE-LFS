<?xml version="1.0" encoding="UTF-8"?>
<?use version="6.0.0"?>
<plugin name="ConverterXMI" version="6.0.0-r1">
	<actions>
		<action label="Export to XMI" icon="resources/export-2.png"
			class="main.plugin.gui.ActionExportXMI" tooltip="Export to XMI"
			menu="XMI" menuitem="Export to XMI" toolbaritem="plugins"
			id="main.plugin.gui.ActionExportXMI">
		</action>
		<action label="Import from XMI" icon="resources/import-2.png"
			class="main.plugin.gui.ActionImportXMI" tooltip="Import from XMI"
			menu="XMI" menuitem="Import from XMI" toolbaritem="plugins"
			id="main.plugin.gui.ActionImportXMI">
		</action>
	</actions>
	<commands>
		<command class="main.plugin.cmd.ExportXMICmd"
			id="main.plugin.cmd.ExportXMICmd" shellcmd="export xmi"
			label="Export to XMI" help="Exports USE Model to XMI File">
		</command>
		<command class="main.plugin.cmd.ImportXMICmd"
			id="main.plugin.cmd.ImportXMICmd" shellcmd="import xmi"
			label="Import from XMI" help="Imports UML Model from XMI File">
		</command>
	</commands>
</plugin>